<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane 
	maxHeight="-Infinity" 
	maxWidth="-Infinity" 
	minHeight="-Infinity" 
	minWidth="-Infinity" 
	prefHeight="400.0" 
	prefWidth="600.0" 
	xmlns="http://javafx.com/javafx/21" 
	xmlns:fx="http://javafx.com/fxml/1" 
	fx:controller="application.HomeController"
>
   <children>
      <Button 
      	fx:id="testButton" 
      	layoutX="280.0" 
      	layoutY="188.0" 
      	mnemonicParsing="false" 
      	onAction="#handleClick" 
      	text="Click Me!" 
      />
      <Button
      	fx:id="nextPageButton"
      	layoutX="280.0" 
      	layoutY="288.0" 
      	mnemonicParsing="false" 
      	onAction="#onClickNextPage" 
      	text="Next page!" 
      />
   </children>
</AnchorPane>
